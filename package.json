{
  "name": "@d-leb/swc-jest",
  "version": "0.1.2",
  "description": "A library for testing TypeScript and JavaScript using SWC and Jest",
  "author": "David LeBlanc <github@david-leblanc.com>",
  "license": "MIT",
  "engines": {
    "node": ">=16.17.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/d-leb/swc-jest.git"
  },
  "packageManager": "yarn@3.3.1",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "bin",
    "defaults",
    "scripts",
    "index.js",
    "index.d.ts"
  ],
  "main": "index.js",
  "types": "index.d.ts",
  "bin": "bin/swc-jest.mjs",
  "scripts": {
    "lint": "yarn prettier",
    "prettier": "prettier --check 'scripts/**/*.mjs' 'bin/**/*.mjs' 'index.js' 'index.d.ts'",
    "prettier:fix": "prettier --write 'scripts/**/*.mjs' 'bin/**/*.mjs' 'index.js' 'index.d.ts'"
  },
  "dependencies": {
    "@jest/globals": "29.5.0",
    "@swc/core": "1.3.62",
    "@swc/helpers": "0.5.1",
    "@swc/jest": "0.2.26",
    "@types/mocha": "10.0.1",
    "cross-spawn": "7.0.3",
    "jest": "29.5.0",
    "jest-environment-jsdom": "29.5.0"
  },
  "devDependencies": {
    "prettier": "^2.8.8"
  }
}
